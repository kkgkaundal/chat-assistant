{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "arrowParens": "always",
  "printWidth": 200,
  "jsxSingleQuote": false,
  "endOfLine": "lf",
  "proseWrap": "always",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed",
  "insertPragma": false,
  "requirePragma": false,
  "embeddedLanguageFormatting": "auto",
  "bracketSameLine": true,
  "singleAttributePerLine": true
}
